{
  "expo": {
    "name": "megamaxdevlopment",
    "slug": "megamaxdevlopment",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/logo-bg-square.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/logo-bg-square.png",
      "resizeMode": "contain",
      "backgroundColor": "#000"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/logo-bg-square.png",
        "backgroundColor": "#000"
      },
      "package": "com.megamaxdevlopment"
    },
    "web": {
      "favicon": "./assets/images/logo-bg-square.png"
    }
  }
}
