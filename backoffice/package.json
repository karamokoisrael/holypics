{
  "name": "backoffice",
  "version": "1.0.0",
  "description": "a cool directus extension to manager exported sql file in the database directory",
  "main": "index.js",
  "scripts": {
    "dev": "nodemon --watch src/**/* --ext ts,json,vue --ignore 'src/**/*.spec.ts' --exec \"tsc -p tsconfig.json && node node_modules/directus/dist/start\"",
    "start": "npx directus bootstrap && npx directus start",
    "build": "directus-extension-builder build && tsc -p tsconfig.json && rm snapshot.yaml && npx directus schema snapshot snapshot.yaml && cp -r src/templates extensions/",
    "migrate": "npx directus bootstrap && npx directus schema apply -y snapshot.yaml",
    "init": "npx directus bootstrap",
    "create-extension": "npm init directus-extension",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@directus/sdk": "^9.7.0",
    "@nodejscart/mysql-query-builder": "^1.3.1",
    "bcryptjs": "^2.4.3",
    "directus": "^9.5.0",
    "directus-extension-display-link": "^1.5.2",
    "ejs": "^3.1.6",
    "joi": "^17.5.0",
    "jsonwebtoken": "^8.5.1",
    "liquidjs": "^9.34.0",
    "lodash": "^4.17.21",
    "morgan": "^1.10.0",
    "mysql": "^2.18.1",
    "mysql-import": "^5.0.21",
    "mysqldump": "^3.2.0",
    "nodemailer": "^6.7.2",
    "pg": "^8.7.1",
    "source-map-support": "^0.5.20",
    "uuid": "^8.3.2",
    "vue-command": "^23.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.15.8",
    "@babel/preset-env": "^7.15.8",
    "@types/bcryptjs": "^2.4.2",
    "@types/express": "^4.17.13",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/lodash": "^4.14.178",
    "@types/morgan": "^1.9.3",
    "@types/mysql": "^2.15.20",
    "@types/node": "^17.0.13",
    "@types/uuid": "^8.3.4",
    "@types/webpack-node-externals": "^2.5.3",
    "babel-loader": "^8.2.3",
    "babel-plugin-source-map-support": "^2.1.3",
    "directus-ext": "^0.0.4",
    "fork-ts-checker-webpack-plugin": "^6.4.0",
    "ts-loader": "^9.2.6",
    "ts-node": "^10.4.0",
    "tsconfig-paths-webpack-plugin": "^3.5.1",
    "typescript": "^4.5.5",
    "webpack": "^5.61.0",
    "webpack-cli": "^4.9.1",
    "webpack-merge": "^5.8.0",
    "webpack-node-externals": "^3.0.0"
  }
}
