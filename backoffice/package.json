{
  "name": "backoffice",
  "version": "1.0.0",
  "description": "a cool directus extension to manager exported sql file in the database directory",
  "main": "index.js",
  "scripts": {
    "dev": "nodemon --watch 'src/**/*' --ext 'ts,json' --ignore 'src/**/*.spec.ts' --exec 'tsc -p tsconfig.json; node node_modules/directus/dist/start'",
    "start": "npx directus bootstrap && npx directus start",
    "build": "npx directus bootstrap",
    "build-local": "directus-extension-builder build && tsc -p tsconfig.json && rm snapshot.yaml && npx directus schema snapshot snapshot.yaml && cp -r src/templates extensions/",
    "migrate": "npx directus bootstrap && npx directus schema apply -y snapshot.yaml",
    "init": "npx directus bootstrap",
    "create-extension": "npm init directus-extension",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
