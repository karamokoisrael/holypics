{
	"name": "directus-extension-mgx-display-local-currency",
	"version": "1.0.0",
	"keywords": [
		"directus",
		"directus-extension",
		"directus-custom-display"
	],
	"directus:extension": {
		"type": "display",
		"path": "dist/index.js",
		"source": "src/index.ts",
		"host": "^9.5.0"
	},
	"scripts": {
		"build": "npx directus-extension build",
		"dev": "nodemon --watch src/**/* --ext ts,js,json,vue --exec \"npx directus-extension build && mv dist/* ./ && cd ../../../ && npx directus start\""
	},
	"devDependencies": {
		"@directus/extensions-sdk": "9.5.0",
		"typescript": "^4.5.5",
		"vue": "^3.2.31"
	},
	"dependencies": {
		"vue-i18n": "^9.1.9"
	}
}
